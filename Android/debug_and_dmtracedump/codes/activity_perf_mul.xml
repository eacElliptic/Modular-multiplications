<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="fr.android.bri.test_profiling.MainActivity">

    <TextView
        android:id="@+id/nbiter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true" />

    <SeekBar
        android:id="@+id/seekBar1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_below="@id/nbiter"
        android:layout_marginTop="32dp"
        android:max="30" />

    <RadioGroup
        android:id="@+id/opGoup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/seekBar1"
        android:layout_marginTop="20dp">

        <RadioButton
            android:id="@+id/radioMult"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="true"
            android:text="@string/multiplication" />

        <RadioButton
            android:id="@+id/radioAdd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/addition" />

    </RadioGroup>

    <Button
        android:id="@+id/bt192"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/opGoup"
        android:layout_marginTop="25dp"
        android:onClick="go"
        android:text="@string/bt192" />

    <Button
        android:id="@+id/bt269"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/bt192"
        android:layout_toRightOf="@id/bt192"
        android:onClick="go"
        android:text="@string/bt269" />

    <Button
        android:id="@+id/bt256"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/bt269"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/bt269"
        android:onClick="go"
        android:text="@string/bt256" />

    <Button
        android:id="@+id/bt358"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/bt256"
        android:layout_toRightOf="@id/bt256"
        android:onClick="go"
        android:text="@string/bt358" />


    <Button
        android:id="@+id/bt320"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/bt192"
        android:onClick="go"
        android:text="@string/bt320" />

    <Button
        android:id="@+id/bt448"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/bt320"
        android:layout_toRightOf="@id/bt320"
        android:onClick="go"
        android:text="@string/bt448" />

    <Button
        android:id="@+id/bt384"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/bt448"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/bt448"
        android:onClick="go"
        android:text="@string/bt384" />

    <Button
        android:id="@+id/bt538"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/bt384"
        android:layout_toRightOf="@id/bt384"
        android:onClick="go"
        android:text="@string/bt538" />


    <Button
        android:id="@+id/bt627"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/bt448"
        android:layout_toRightOf="@id/bt320"
        android:onClick="go"
        android:text="@string/bt627" />


    <Button
        android:id="@+id/bt512"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/bt384"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/bt627"
        android:onClick="go"
        android:text="@string/bt512" />

    <Button
        android:id="@+id/bt717"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/bt512"
        android:layout_toRightOf="@id/bt512"
        android:onClick="go"
        android:text="@string/bt717" />


    <Button
        android:id="@+id/bt270"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/bt627"
        android:layout_marginTop="20dp"
        android:onClick="go"
        android:text="@string/bt270" />

    <Button
        android:id="@+id/bt373"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/bt270"
        android:layout_toRightOf="@id/bt270"
        android:onClick="go"
        android:text="@string/bt373" />

    <Button
        android:id="@+id/bt536"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/bt373"
        android:layout_marginLeft="10dp"
        android:layout_toRightOf="@id/bt373"
        android:onClick="go"
        android:text="@string/bt536" />

    <Button
        android:id="@+id/bt713"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/bt536"
        android:layout_toRightOf="@id/bt536"
        android:onClick="go"
        android:text="@string/bt713" />

    <!--<Button-->
    <!--android:id="@+id/bt716"-->
    <!--android:layout_width="wrap_content"-->
    <!--android:layout_height="wrap_content"-->
    <!--android:layout_alignBottom="@id/bt384"-->
    <!--android:layout_toRightOf="@id/bt384"-->
    <!--android:onClick="go"-->
    <!--android:text="@string/bt716" />-->

</RelativeLayout>
